{
  "name": "high5",
  "version": "1.0.0",
  "description": "(eventually) spec-compliant html 5 parser",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha -R min && jshint index.js test/html5lib-tokenizer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fb55/high5.git"
  },
  "keywords": [
    "html5",
    "parser",
    "tokenizer"
  ],
  "author": "Felix BÃ¶hm <me@feedic.com>",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/fb55/high5/issues"
  },
  "homepage": "https://github.com/fb55/high5",
  "dependencies": {
    "entities": "~1.1.1"
  },
  "devDependencies": {
    "mocha": "^5.0.1",
    "jshint": "2"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "freeze": true,
    "latedef": "nofunc",
    "noarg": true,
    "nonbsp": true,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "trailing": true,
    "eqnull": true,
    "proto": true,
    "smarttabs": true,
    "node": true,
    "globals": {
      "describe": true,
      "it": true
    },
    "esnext": true
  }
